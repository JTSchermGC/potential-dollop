    <validationRules>
        <fullName>AmountValue</fullName>
        <active>true</active>
        <errorConditionFormula>npe03__Amount__c &lt;= 0</errorConditionFormula>
        <errorDisplayField>npe03__Amount__c</errorDisplayField>
        <errorMessage>Please add correct value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Installments</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( npe03__Installments__c )</errorConditionFormula>
        <errorDisplayField>npe03__Installments__c</errorDisplayField>
        <errorMessage>Complete this field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>InstallmentsValue</fullName>
        <active>true</active>
        <errorConditionFormula>npe03__Installments__c &lt;= 0</errorConditionFormula>
        <errorDisplayField>npe03__Installments__c</errorDisplayField>
        <errorMessage>Please add correct value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Parish_Id_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL( npe03__Schedule_Type__c , &apos;Divide By&apos;),
ISBLANK( Households__c )
)</errorConditionFormula>
        <errorDisplayField>Households__c</errorDisplayField>
        <errorMessage>Please select a Parish ID.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Schedule_Type_Error</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(npe03__Schedule_Type__c) &lt;&gt; if( TEXT(npe03__Recurring_Donation_Campaign__r.Type) &lt;&gt; &apos;Pledged Events&apos; &amp;&amp; TEXT(npe03__Recurring_Donation_Campaign__r.Type) &lt;&gt; &apos;High Value&apos;, &apos;Multiply By&apos;, &apos;Divide By&apos;)</errorConditionFormula>
        <errorMessage>The Selected campaign, fund, or event is not compatible for this type of gift, please correct and try again.</errorMessage>
    </validationRules>
</CustomObject>
