    <validationRules>
        <fullName>enddate</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( EndDate )</errorConditionFormula>
        <errorDisplayField>EndDate</errorDisplayField>
        <errorMessage>This is a required field, please enter a date in the future. If you wish donations to be ongoing the recommended end date should be at least five years from today. The end date is always editable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>startdate</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(StartDate)</errorConditionFormula>
        <errorDisplayField>StartDate</errorDisplayField>
        <errorMessage>This is a required field. Please enter today&apos;s date or a date in the future.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>startdatecheck</fullName>
        <active>true</active>
        <errorConditionFormula>StartDate &lt; TODAY()</errorConditionFormula>
        <errorDisplayField>StartDate</errorDisplayField>
        <errorMessage>Event start date cannot be backdated. Please enter today&apos;s date or a date in the future on which contributions to this campaign will be accepted.</errorMessage>
    </validationRules>
